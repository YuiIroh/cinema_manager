assword\"\n                            placeholder=\"请输入密码\"\n                            show-password\n                        ></el-input>\n                    </el-col>\n                </el-form-item>\n                <!-- 按扭区域 -->\n                <el-form-item class=\"btns\">\n                    <el-button\n                        style=\"font-size: 22px\"\n                        :round=\"true\"\n                        type=\"primary\"\n                        @click=\"login\"\n                    >\n                        <i\n                            class=\"iconfont icon-r-yes\"\n                            style=\"font-size: 22px\"\n                        ></i>\n                        登录\n                    </el-button>\n                </el-form-item>\n            </el-form>\n        </div>\n    </div>\n</template>\n\n<script>\nexport default {\n    name: \"Login\",\n    data() {\n        return {\n            //登录表单数据对象\n            loginForm: {\n                userName: \"\",\n                password: \"\",\n            },\n            //表单验证规则\n            loginFormRules: {\n                //验证用户名\n                userName: [\n                    {\n                        required: true,\n                        message: \"请输入用户名称\",\n                        trigger: \"blur\",\n                    },\n                    {\n                        min: 2,\n                        max: 20,\n                        message: \"长度在2到20个字符之间\",\n                        trigger: \"blur\",\n                    },\n                ],\n                //验证密码\n                password: [\n                    { required: true, message: \"请输入密码\", trigger: \"blur\" },\n                    {\n                        min: 6,\n                        max: 16,\n                        message: \"长度在6到16个字符之间\",\n                        trigger: \"blur\",\n                    },\n                ],\n            },\n        };\n    },\n    methods: {\n    